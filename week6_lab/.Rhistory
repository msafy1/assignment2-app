n = 50
set.seed(0)
x = sort(runif(n, min=-2, max=2))
y = x^3 + rnorm(n)
plot(x, y, type="p")
plot(x, y, type="l")
#The plot with type "l" looks abnormal because "l" creaates lines.
plot(y~x, cex=.5)
plot(y~x, cex=.5)
plot(y~x, cex=.5)
plot(y~x, cex=2)
plot(y~x, cex=.5, title = "Shrunken points")
plot(y~x, cex=2, title = "Expanded points")
plot(y~x, cex=.5, main = "Shrunken points")
plot(y~x, cex=2, main = "Expanded points")
plot(y~x, xlim = c(-1,0))
plot(y~x, xlim = c(-1,1), ylim = c(-1,1))
